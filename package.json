{
  "name": "midasmoon-frontend",
  "version": "1.3.0",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --config ./prettier.config.js --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "start": "ng serve --host 0.0.0.0 --port 3333 --disableHostCheck",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider; ng build",
    "build:dev": "ng build --configuration=development",
    "build:dev2": "ng build --configuration=dev2",
    "build:qa": "ng build --configuration=qa",
    "build:qa2": "ng build --configuration=qa2",
    "build:uat": "ng build --configuration=uat",
    "build:demo": "ng build --configuration=demo",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "playground": "angular-playground",
    "ci:test": "ng test --watch=false --no-progress --browsers=ChromeNoSandboxHeadless",
    "ci:format:check": "prettier --config ./prettier.config.js --list-different \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "build:stats": "ng build --prod --stats-json",
    "stats": "npm run build:stats && npx webpack-bundle-analyzer --port 8889 dist/midasmoon-frontend/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.7",
    "@angular/cdk": "~9.2.3",
    "@angular/cli": "^9.1.15",
    "@angular/common": "~9.1.7",
    "@angular/compiler": "~9.1.7",
    "@angular/core": "~9.1.7",
    "@angular/forms": "~9.1.7",
    "@angular/localize": "~9.1.7",
    "@angular/material": "^9.2.3",
    "@angular/platform-browser": "~9.1.7",
    "@angular/platform-browser-dynamic": "~9.1.7",
    "@angular/router": "~9.1.7",
    "@elastic/apm-rum": "^4.5.1",
    "@ng-bootstrap/ng-bootstrap": "^6.2.0",
    "@ngx-loading-bar/core": "^4.2.0",
    "@ngx-loading-bar/http-client": "^4.2.0",
    "@ngx-loading-bar/router": "^4.2.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^5.0.0",
    "@nicky-lenaers/ngx-scroll-to": "^3.0.1",
    "@types/d3-geo": "^2.0.0",
    "angular-csv-ext": "^1.0.5",
    "angular-resize-event": "^3.0.2",
    "bootstrap": "^4.3.1",
    "chartjs-plugin-style": "^0.5.0",
    "core-js": "^2.6.10",
    "date-fns": "^1.30.1",
    "export-to-csv": "^0.2.1",
    "fast-deep-equal": "^2.0.1",
    "file-saver": "^2.0.2",
    "jquery": "^3.6.0",
    "js-marker-clusterer": "^1.0.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "ng-quill-simple": "0.0.5",
    "ngx-bootstrap": "^6.2.0",
    "ngx-quicklink": "^0.1.5",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "tsutils": "^3.17.1",
    "uuid": "^3.3.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.6",
    "@angular/compiler-cli": "~9.1.7",
    "@angular/language-service": "~9.1.7",
    "@angularclass/hmr": "^2.1.3",
    "@ngxs/hmr-plugin": "^3.5.1",
    "@ngxs/store": "^3.5.1",
    "@schuchard/prettier": "^3.1.0",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.170",
    "@types/node": "^12.11.1",
    "@types/uuid": "^3.4.6",
    "angular-playground": "^7.0.3",
    "codelyzer": "^5.1.2",
    "husky": "2.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "8.1.7",
    "prettier": "1.19.1",
    "protractor": "^7.0.0",
    "rxjs-tslint-rules": "^4.26.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint && ng build --prod"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts,html,component.html}": [
      "npm run prettier",
      "git add"
    ]
  },
  "resolutions": {
    "https-proxy-agent": "^3.0.0",
    "mem": "^4.0.0",
    "serialize-javascript": "^2.1.1"
  }
}
